cat > .gitignore << 'EOF'
# Django
__pycache__/
*.py[cod]
*.sqlite3
*.log
media/
staticfiles/
.env
.env.*

# Byte-compiled
*.pyc

# IDEs
.vscode/
.idea/

# Docker
*.pid
*.sock
*.pid.lock

# Redis, DB, etc.
data/
postgres_data/
redis_data/

# Coverage / tests
htmlcov/
.coverage
.pytest_cache/

# AWS/Secrets
*.pem
*.key
EOF